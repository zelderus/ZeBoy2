;-----------------------------------
;	Сложение 16 бит
;	IN:
;		R1,R2 - x; R3,R4 - y
;	OUT:
;		R1(high),R2(low)
;-----------------------------------
MATH_ADD16:
	; low
	MOV A, R2
	ADDC A, R4
	MOV R2, A
	; hight
	MOV A, R1
	ADDC A, R3
	MOV R1, A
	; result R1,R2
	RET
	
	
	
	